<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CalendarFont" parent="@android:style/TextAppearance.Medium">
        <item name="android:textColor">#000</item>
        <item name="android:textSize">24pt</item>
    </style>

    <color name="CalendarDisabledColor">#000</color>
    <color name="CalendarBorderColor">#000</color>
    <color name="CalendarTextColor">#000</color>
    <color name="CalendarResTextColor">#000</color>
    <color name="StatusFreeColor">#000</color>
    <color name="StatusReservedColor">#000</color>
    <color name="Transparent">#fff</color>
    <color name="CalendarMarkerReservedColor">#000</color>
    <color name="CalendarMarkerFreeColor">#fff</color>
    <color name="WhiteColor">#ffffff</color>

    <style name="Theme.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@color/WhiteColor</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:windowIsFloating">true</item> -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <color name="TextColor">#000</color>
    <color name="TimeSpanTextColor">#000</color>

    <style name="TimeSpanTimeStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/TimeSpanTextColor</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="DigitalClockStyle">
        <item name="android:textColor">@color/TimeSpanTextColor</item>
        <item name="android:textSize">32sp</item>
    </style>

    <color name="FutuLightGreen">#000</color>
    <color name="ReserveBackground">#fff</color>
    <color name="ReserveLine">#000</color>
    <color name="TimeBarTickColor">#000</color>
    <color name="ButtonTextNormal">#000</color>
    <color name="ButtonTextActive">#fff</color>
    <color name="ButtonTextDisabled">#000</color>
    <color name="TextInfoColor">#000</color>
    <color name="CalendarWeekTextColor">#000</color>
    <color name="MenuButtonBackgroundColor">#fff</color>
    <color name="MenuButtonPressedColor">#000</color>
    <color name="MenuButtonInactiveColor">#000</color>

    <color name="TrafficLightFree">#fff</color>
    <color name="TrafficLightYellow">#a8a8a8</color>
    <color name="TrafficLightReserved">#545454</color>
    <color name="TrafficLightText">#fff</color>
    <color name="TrafficLightGreenButtonPressed">#fff</color>
    <color name="TrafficLightGreenButtonPressedText">#000</color>
    <color name="TrafficLightYellowButtonText">#fff</color>
    <color name="TrafficLightYellowButtonPressed">#fff</color>
    <color name="TrafficLightYellowButtonPressedText">#fff</color>

</resources>